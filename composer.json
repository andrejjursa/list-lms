{
  "name": "andrejjursa/list-lms",
  "description": "LIST learning management system for Department of Applied Informatics, Faculty of Mathematics, Physics and Informatics, Comenius University in Bratislava, Slovakia.",
  "authors": [
    {
      "name": "Andrej Jursa",
      "email": "dragoo@thelambda.sk"
    }
  ],
  "require": {
    "php": "7.1.*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pcntl": "*",
    "ext-zip": "*",
    "mathjax/mathjax": "^2.7",
    "php-amqplib/php-amqplib": "^3.1",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "*",
    "ergebnis/composer-normalize": "^2.8",
    "phpcompatibility/php-compatibility": "*",
    "rector/rector": "^0.12",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Application\\Interfaces\\": "application/interface/",
      "Application\\Services\\": "application/services/",
      "Application\\ModelTraits\\": "application/modelTraits/",
      "Application\\MetaClass\\": "application/metaclass/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
